<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Listrak</servlet-name>
        <servlet-class>Listrak.Listrak</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AMZOrders</servlet-name>
        <servlet-class>AMZ.AMZOrders</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>YahooOrderUpdate</servlet-name>
        <servlet-class>Yahoo.Order.Update.YahooOrderUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListrakModuleSettings</servlet-name>
        <servlet-class>Listrak.ListrakModuleSettings</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AmazonModuleSettings</servlet-name>
        <servlet-class>AMZ.AmazonModuleSettings</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AmazonInventoryModuleSettings</servlet-name>
        <servlet-class>AMZ.AmazonInventoryModuleSettings</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Listrak</servlet-name>
        <url-pattern>/Listrak_Data_Feed</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AMZOrders</servlet-name>
        <url-pattern>/AMZOrders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>YahooOrderUpdate</servlet-name>
        <url-pattern>/YahooOrderUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListrakModuleSettings</servlet-name>
        <url-pattern>/ListrakModuleSettings</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AmazonModuleSettings</servlet-name>
        <url-pattern>/AmazonModuleSettings</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AmazonInventoryModuleSettings</servlet-name>
        <url-pattern>/AmazonInventoryModuleSettings</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <listener>
        <listener-class>com.quartz.QuartzListener</listener-class>
    </listener>
</web-app>
